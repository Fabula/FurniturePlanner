<?xml version="1.0" encoding="utf-8"?>
<Objects 
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns="http://www.spicefactory.org/parsley"
	xmlns:spicefactory="http://www.spicefactory.org/parsley"  
	xmlns:views="views.*" 
	xmlns:command="command.*" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:business="business.*" 
	xmlns:presentationmodel="views.presentationmodel.*"
	xmlns:model="model.*" 
	xmlns:userPage="views.userPage.*"> 
	<fx:Declarations>
		<!--views-->
		<views:RegistrationPanel />
		<!--presentation-->
		<presentationmodel:LoginPanelPM />
		<presentationmodel:RegistrationPM />
		<presentationmodel:ToolAreaPM />
		<presentationmodel:MainPagePM />
		
		<userPage:CreateNewProjectPanelPM />
		<business:MainWindowDelegate />
		<business:RoomDelegate />
		<command:CreateUserCommand />
		<command:CreateProjectCommand />
		<!--models-->
		<!--commands-->
		<spicefactory:DynamicCommand type="{CreateUserCommand}" />
		<spicefactory:DynamicCommand type="{NavigateToMainWindow}" />
		<spicefactory:DynamicCommand type="{CreateSessionCommand}" />
		<spicefactory:DynamicCommand type="{CreateProjectCommand}" />
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import command.CreateUserCommand;
			import command.CreateSessionCommand;
			import command.CreateProjectCommand;
			import command.NavigateToMainWindow;
			import business.MainWindowDelegate;
			import business.RoomDelegate;
			import views.presentationmodel.ToolAreaPM;
			import views.presentationmodel.RegistrationPM;
			import views.presentationmodel.MainPagePM;
			import views.userPage.CreateNewProjectPanelPM;
		]]>
	</fx:Script>
</Objects>